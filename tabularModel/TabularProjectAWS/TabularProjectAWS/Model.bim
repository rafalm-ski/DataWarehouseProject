        "measures": [
          {
            "name": "Total Sales",
            "expression": " SUM(Sale[Total Including Tax])",
            "kpi": {
              "targetExpression": "'Sale'[Target Sales]",
              "statusGraphic": "Traffic Light - Single",
              "statusExpression": [
                "var x='Sale'[Total Sales]/'Sale'[_Total Sales Goal] return",
                "            if(ISBLANK(x),BLANK(),",
                "                    If(x<0.9,-1,",
                "\t                If(x<1,0,1)",
                "                )",
                "            )",
                "            "
              ],
              "annotations": [
                {
                  "name": "GoalType",
                  "value": "Measure"
                },
                {
                  "name": "KpiStatusType",
                  "value": "Linear"
                },
                {
                  "name": "KpiThresholdType",
                  "value": "Percentage"
                },
                {
                  "name": "KpiThresholdOrdering",
                  "value": "Ascending"
                },
                {
                  "name": "KpiThresholdCount",
                  "value": "2"
                },
                {
                  "name": "KpiThreshold_0",
                  "value": "90"
                },
                {
                  "name": "KpiThreshold_1",
                  "value": "100"
                }
              ]
            }
          },
          {
            "name": "Quantity Sold",
            "expression": " SUM(Sale[Quantity])"
          },
          {
            "name": "Total Profit",
            "expression": " SUM (Sale[Profit])"
          },
          {
            "name": "Sales 2015",
            "expression": [
              "",
              "CALCULATE (",
              "    [Total Sales],",
              "    'Date'[Calendar Year] = 2015",
              ")"
            ]
          },
          {
            "name": "Sales 2016",
            "expression": [
              "",
              "CALCULATE (",
              "    [Total Sales],",
              "    'Date'[Calendar Year] = 2016",
              ")"
            ]
          },
          {
            "name": "YoY Growth",
            "expression": [
              "",
              "VAR PrevYear = [Sales 2015]",
              "VAR CurrYear = [Sales 2016]",
              "RETURN",
              "IF (",
              "    PrevYear = 0,",
              "    BLANK (),",
              "    DIVIDE ( CurrYear - PrevYear, PrevYear )",
              ")"
            ]
          },
          {
            "name": "Target Sales",
            "expression": [
              "",
              "AVERAGEX(",
              "    VALUES(Customer[Customer]),",
              "    [Sales 2015]",
              ") * 1.1"
            ]
          }
        ]
